<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

<package name="/" extends="struts-default">
<action name="loginAction" class="com.hearthealth.run.account.LoginAction">
	<result name="success">/pages/account/homepage_user.jsp</result>
	<result name="error">/pages/account/loginFailure.jsp</result>
	<result name="admin">/pages/account/homepage_admin.jsp</result>
</action>

<action name="registerUser" class="com.hearthealth.run.account.RegisterAction">
	<result name="success">/pages/account/register_success.jsp</result>
	<result name="error">/pages/account/register_failure.jsp</result>
</action>

</package>

</struts>